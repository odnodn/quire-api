{{!--
  This partial renders the main page content
  @api public
--}}
<div id="page" class="drawer-layout drawer-slide-right-large">
  <header class="header-wrap navbar navbar-default navbar-fixed-top">
    <div id="header" class="container">
      <div id="quire-logo" class="navbar-header">
        <h1 class="quire-logo-icon">
          <em>
            <a href="/" title="Quire">
              <img src="https://quire.io/s/img/website/quire_logo.svg" alt="Quire">
            </a>
          </em>
        </h1>
        <h3 class="app-home-icon"><a href="/doc/api/v2/">API Documentation</a></h3>
      </div>
      <nav class="navbar-collapse bs-navbar-collapse collapse clearfix">
        <ul id="header-icons" class="nav navbar-nav navbar-right">
          <li class="item workspace">
            <a href="/w" class="b white">GO TO YOUR PROJECT</a>
          </li>
        </ul>
      </nav>
    </div>
  </header>
  <div id="sidebar">
    {{>layout/nav}}
  </div>
  {{!layout/topbar}}
  <div id="docs" class="row collapse expanded drawer" data-drawer>
    <button class="floating-menu-icon" type="button" data-drawer-slide="right">
      <span class="hamburger"></span>
    </button>
    <div class="example-box doc-content"></div>
    {{>layout/content}}
  </div>
</div>
